{
  "name": "react_exercise",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/react_exercise.cjs.js",
  "module": "./dist/react_exercise.es.mjs",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/react_exercise.es.mjs",
      "require": "./dist/react_exercise.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "prettier:check": "prettier --check src",
    "build": "vite build ",
    "preview": "vite preview",
    "mock": "json-server db.json -p 3001"
